#!/bin/sh

export action=$1
export country=$2
case $action in
	"set")
		export SRC_DEFAULT=interaxtelephony/default
		export DST_DEFAULT=interaxtelephony/$country
	;;
	
	"unset")
		export SRC_DEFAULT=interaxtelephony/$country
		export DST_DEFAULT=interaxtelephony/default
	;;
	
	*) 
		echo "Unknown DEFAULT action."
	;;
esac
		
		
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/callcenter/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/callingcard/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/conference/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/activation/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/emergency/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/forward/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/incoming/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/outgoing/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/ippbx/von/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/proxy/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/test/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/voicetraffic/forward/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/voicetraffic/outgoing/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/voicetraffic/voipforward/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/voicetraffic/von/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agis/did/forward/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agiserver.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/agiserver.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AGI_SERVER_DIR_CONFIG/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/callingcard/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/callingcard/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/callingcard/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/ippbx/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/ippbx/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/ippbx/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/voicetraffic/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/voicetraffic/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/voicetraffic/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/did/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/did/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amis/did/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amiserver.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $AMI_DIR_CONFIG/amiserver.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/IpPbxProvisionerThread/config/ippbxprovisionerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/IpPbxProvisionerThread/config/ippbxprovisionerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/IpPbxProvisionerThread/config/ippbxprovisionerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/IpPbxProvisionerThread/config/ippbxprovisionerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/IpPbxProvisionerThread/config/log4j.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/ITSecurityManagerThread/config/itsecuritymanagerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/ITSecurityManagerThread/config/itsecuritymanagerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/ITSecurityManagerThread/config/itsecuritymanagerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/ITSecurityManagerThread/config/itsecuritymanagerthread.properties
$SUDO_COMMAND $REPLACE_COMMAND $SRC_DEFAULT $DST_DEFAULT -- $INTERAX_TELEPHONY_APPS_DIR/ITSecurityManagerThread/config/log4j.properties



