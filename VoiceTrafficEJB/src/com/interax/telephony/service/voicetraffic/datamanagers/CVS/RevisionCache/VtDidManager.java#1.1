package com.interax.telephony.service.voicetraffic.datamanagers;

import java.sql.Connection;

import com.interax.persistence.datamanagers.DataManager;
import com.interax.telephony.service.voicetraffic.data.VtDid;

public class VtDidManager extends DataManager
{
	
	public VtDidManager(String dataSourceName) {
		super(dataSourceName);
		init();
	}
	
	public VtDidManager(Connection controlledConnection) {
		super(controlledConnection);
		init();
	}
	
	private void init(){
		this.allowInsertUnsignedNegatives = true;
	}

	@Override
	public Object getObject() {
		return new VtDid();
	}

	@Override
	public String getPrimaryKey(Object obj) {
		return "id='"+obj+"'";
	}

	@Override
	public String getTableName() {
		return "vt_did";
	}

}