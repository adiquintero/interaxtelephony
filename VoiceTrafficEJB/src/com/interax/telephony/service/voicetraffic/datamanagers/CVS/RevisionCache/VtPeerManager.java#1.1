package com.interax.telephony.service.voicetraffic.datamanagers;

import java.sql.Connection;

import com.interax.persistence.datamanagers.DataManager;
import com.interax.telephony.service.voicetraffic.data.VtPeer;

public class VtPeerManager extends DataManager
{

	public VtPeerManager(String dataSourceName) {
		super(dataSourceName);
		init();
	}
	
	public VtPeerManager(Connection controlledConnection) {
		super(controlledConnection);
		init();
	}
	
	private void init(){
		this.allowInsertUnsignedNegatives = true;
	}
	
	@Override
	public Object getObject() {
		return new VtPeer();
	}

	@Override
	public String getPrimaryKey(Object obj) {
		return "id='"+obj+"'";
	}

	@Override
	public String getTableName() {
		return "vt_peer";
	}

}