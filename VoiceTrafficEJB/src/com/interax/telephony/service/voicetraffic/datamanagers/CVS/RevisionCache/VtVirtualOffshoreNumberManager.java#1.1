package com.interax.telephony.service.voicetraffic.datamanagers;

import java.sql.Connection;

import com.interax.persistence.datamanagers.DataManager;
import com.interax.telephony.service.voicetraffic.data.VtVirtualOffshoreNumber;

public class VtVirtualOffshoreNumberManager extends DataManager
{

	public VtVirtualOffshoreNumberManager(String dataSourceName) {
		super(dataSourceName);
		init();
	}

	public VtVirtualOffshoreNumberManager(Connection controlledConnection) {
		super(controlledConnection);
		init();
	}
	
	private void init(){
		this.allowInsertUnsignedNegatives = true;
	}
	
	@Override
	public Object getObject() {
		return new VtVirtualOffshoreNumber();
	}

	@Override
	public String getPrimaryKey(Object obj) {
		return "id='"+obj+"'";
	}

	@Override
	public String getTableName() {
		return "vt_virtual_offshore_number";
	}

}