package com.interax.telephony.service.voicetraffic.datamanagers;

import java.sql.Connection;

import com.interax.persistence.datamanagers.DataManager;
import com.interax.telephony.service.voicetraffic.data.VtDidStep;

public class VtDidStepManager extends DataManager
{

	public VtDidStepManager(String dataSourceName) {
		super(dataSourceName);
		init();
	}
	
	public VtDidStepManager(Connection controlledConnection) {
		super(controlledConnection);
		init();
	}
	
	private void init(){
		this.allowInsertUnsignedNegatives = true;
	}
	
	@Override
	public Object getObject() {
		return new VtDidStep();
	}

	@Override
	public String getPrimaryKey(Object obj) {
		return "id='"+obj+"'";
	}

	@Override
	public String getTableName() {
		return "vt_did_step";
	}

}