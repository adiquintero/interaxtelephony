package com.interax.telephony.service.callingcard.data;

import java.io.Serializable;

import com.interax.telephony.service.data.ServiceRequestData;

public class CallingCardRequestData extends ServiceRequestData implements Serializable, CallingCardData {
	
	private static final long serialVersionUID = 1L;
	private String ani;
	private String dni;
	private String dnid;
	private Long pinSerial;
	private String cdrId;
	
	private int serviceType;
	private int accessType;
	
	public String getAni() {
		return ani;
	}
	public void setAni(String ani) {
		this.ani = ani;
	}
	public String getDni() {
		return dni;
	}
	public void setDni(String dni) {
		this.dni = dni;
	}
	public String getDnid() {
		return dnid;
	}
	public void setDnid(String dnid) {
		this.dnid = dnid;
	}
	public Long getPinSerial() {
		return pinSerial;
	}
	public void setPinSerial(Long pinSerial) {
		this.pinSerial = pinSerial;
	}
	public String getCdrId() {
		return cdrId;
	}
	public void setCdrId(String cdrId) {
		this.cdrId = cdrId;
	}
	
	public CallingCardAccessType getAccessType() {
		return CallingCardAccessType.values()[this.accessType];
	}
	public void setAccessType(CallingCardAccessType accessType) {
		this.accessType = accessType.ordinal();
	}
	public void setAccessType(String accessType) {
		this.accessType = CallingCardAccessType.valueOf(accessType).ordinal();
	}

	public CallingCardServiceType getServiceType() {
		return CallingCardServiceType.values()[this.serviceType];
	}
	public void setServiceType(CallingCardServiceType serviceType) {
		this.serviceType = serviceType.ordinal();
	}
	public void setServiceType(String serviceType) {
		this.serviceType = CallingCardServiceType.valueOf(serviceType).ordinal();
	}
	@Override
	public String toString() {
		return "CallingCardRequestData{pinSerial="+this.pinSerial+",ani="+this.ani+",cdrId="+this.cdrId+",dni="+this.dni+",dnId="+this.dnid+",accessType="+this.accessType+"}";
	}

}
